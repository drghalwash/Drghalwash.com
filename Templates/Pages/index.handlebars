	<script>
		const toggleButton = document.getElementById('toggle-button');
		const menu = document.getElementById('menu');

		// Toggle the menu visibility on button click
		toggleButton.addEventListener('click', (event) => {
			event.stopPropagation(); // Prevent click from propagating to the document
			menu.classList.toggle('active');
		});

		// Close the menu when clicking outside of it
		document.addEventListener('click', (event) => {
			if (!menu.contains(event.target) && !toggleButton.contains(event.target)) {
				menu.classList.remove('active'); // Remove the active class if clicking outside
			}
		});

		// Function to reset menu visibility for larger screens
		const resetMenu = () => {
			if (window.innerWidth > 768) {
				menu.classList.remove('active'); // Remove the active class in desktop view
			}
		};

		// Listen for window resize events
		window.addEventListener('resize', resetMenu);

		// Initial check to reset menu on page load
		resetMenu();
	</script>



	<script>
		document.addEventListener("DOMContentLoaded", function () {
			const mediaEntries = document.querySelectorAll(".media-entry");
			const showMoreBtn = document.getElementById("show-more-btn");
			const initialVisibleCount = 8; // Number of entries to show initially
			let currentlyVisibleCount = initialVisibleCount;

			// Hide all entries beyond the initial visible count
			mediaEntries.forEach((entry, index) => {
				if (index >= initialVisibleCount) {
					entry.style.display = "none";
				}
			});

			// Show more entries when the button is clicked
			showMoreBtn.addEventListener("click", function () {
				const hiddenEntries = Array.from(mediaEntries).filter(
					(entry) => entry.style.display === "none"
				);

				hiddenEntries.slice(0, initialVisibleCount).forEach((entry) => {
					entry.style.display = "";
				});

				currentlyVisibleCount += initialVisibleCount;

				// Hide the button if all entries are visible
				if (currentlyVisibleCount >= mediaEntries.length) {
					showMoreBtn.style.display = "none";
				}
			});
		});
	</script>

